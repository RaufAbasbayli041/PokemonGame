{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  //RAUFABASBAYLI\\SQLEXPRESS
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Server=RAUFABASBAYLI\\SQLEXPRESS;Database=PokemonGoo;Trusted_Connection=True;TrustServerCertificate=True;",
    // "Default": "Server=GGFTGPC11\\SQLEXPRESS;Database=PokemonGoo;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  
    "Serilog": {
      "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "System": "Warning"
        }
      },

      "WriteTo": [
        {
          "Name": "Console"
        },
        {
          "Name": "File",
          "Args": {
            "path": "Logs/applog-.txt",
            "rollingInterval": "Day"
          }
        }
      ],
      "Enrich": [ "FromLogContext"],
      "Properties": {
        "ApplicationName": "Your ASP.NET Core App"
      }
    }
  
}
